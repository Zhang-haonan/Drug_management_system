#include<iostream>
using namespace std;
int fun(int a,int b)
{
	int c;
	c=(a%10)*1000+(b%10)*100+(a/10)*10+b/10;
	return c;
}
void main()
{
	int a,b;
	int c;
	cin>>a>>b;
	c=fun(a,b);
	cout<<c<<endl;
}
#include<iostream>
using namespace std;
void fun(int a,int b,int &c)
{
	c=(a%10)*1000+(b%10)*100+(a/10)*10+b/10;
	
}
void main()
{
	int a,b;
	int c;
	cin>>a>>b;
	fun(a,b,c);
	cout<<c<<endl;
}
#include<iostream>
using namespace std;
int fun(int *a,int *b)
{
	int c;
	c=(*a%10)*1000+(*b%10)*100+(*a/10)*10+*b/10;
	return c;
}
void main()
{
	int a,b;
	int c;
	cin>>a>>b;
	int fun(int *a,int *b);
	c=fun(&a,&b);
	cout<<c<<endl;
}
